    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.15",
    "@types/winston": "^2.4.4",
    "nodemon-webpack-plugin": "^4.0.8",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-security": "^1.13.0",
