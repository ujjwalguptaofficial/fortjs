{
  "name": "fortjs-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "webpack:watch": "webpack --config webpack.config.js --watch",
    "build": "npm run webpack",
    "start": "npm run webpack:watch && opn http://localhost:4000",
    "install:lib": "npm i ../../fortjs-1.5.7.tgz --no-save",
    "install:build": "npm run install:lib && npm run build",
    "test": "mocha --recursive --exit",
    "build:test": "npm run build && npm run test",
    "install:build:test": "npm run install:lib && npm run build && npm run test"
  },
  "author": "Ujjwal Gupta",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "nodemon-webpack-plugin": "^4.0.6",
    "opn-cli": "^4.0.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "eshtml": "^0.1.0"
  }
